<!DOCTYPE html>
<html>
    <head>
        <title><%= title %></title>
        <link rel="stylesheet" href="/stylesheets/style.css" />
    </head>
    <body>
        <h1><%= title %></h1>
        <p>Welcome to <%= title %></p>
        <% messages.forEach(element => { %>
        <h3><%= element.user %></h3>
        <p><%= element.text %></p>
        <p><%= element.added.toLocaleString() %></p>
        <% }) %>
    </body>
</html>
